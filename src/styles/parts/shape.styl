.shape
	display inline-block
	vertical-align middle
	line-height inherit
	fill currentColor

	&-defs
		visibility hidden
		position absolute
		width 0
		height 0
		overflow hidden

	// Shapes default dimensions
	&-barcode
		size 315px 510px
	&-icon-arrow
		size 5px 9px
	&-icon-books
		size 66px 58px
	&-icon-boost
		size 72px 59px
	&-icon-building
		size 58px 53px
	&-icon-document
		size 319px 333px
	&-icon-document2
		size 46px 54px
	&-icon-mail
		size 282px 122px
	&-icon-phone
		size 237px 292px
	&-icon-sign
		size 64px 46px
	&-icon-target
		size 75px 69px
	&-social-linkedin
		size 39px
	&-logo-ntk
		size 238px 132px
	&-logo-elib
		size 198px 92px
