				li
					span Seznam eZdrojů

	.main
		.container
			.single
				.lead Stavební nikomu druhá než napíná. Vzáleném má, ní mu mi cenám vyšla běžné, útulné mikroorganismy je, do milion pohromou nejvyšší pevnost.

			.well
				.filterbox.view-letternav
					.row
						.col-sm-1
							p Filtr:
						.col-sm-11
							ul.letternav
								li
									span a
								li.letternav-separator
									span |
								each val in ['b', 'c', 'd', 'e', 'f', 'g', 'h', 'c', 'h', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z']
									li
										a(href="#")= val
									li.letternav-separator
										span |


